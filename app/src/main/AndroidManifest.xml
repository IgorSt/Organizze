<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.igorsantos.organizese">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/icone_quadrado"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/icone_redondo"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">

        <meta-data
            android:name="com.google.android.gms.ads.APPLICATION_ID"
            android:value="ca-app-pub-4389755220273266~6629695213"/>

        <activity android:name="com.igorsantos.organizese.activity.activity.Activity_despesas"
            android:theme="@style/temaDespesas"/>
        <activity android:name="com.igorsantos.organizese.activity.activity.Activity_receitas"
